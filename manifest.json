{
    "manifest_version": 2,
    "name": "Chezzy Chrome Extension",
    "description": "Chezzy Chrome extension",
    "author": "Javier Gutierrez",
    "version": "0.1",
    "permissions": ["tabs"],
    "content_scripts": [
      {
        "matches": [
          "<all_urls>"
        ],
        "js": ["scripts/cheesify.js"]
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Cheezzy Chrome Extension"
    },
    "icons": {
      "16": "images/icon16.png",
      "32": "images/icon32.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
}